<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All dosing cohorts
    </h1>

    <a href="/dosing_cohorts/new" class="btn btn-block btn-outline-secondary">
      Add a new dosing cohort
    </a>
  </div>
</div>

<!-- Ransack Code start -->
<div class="row">
  <div class="col-md-12 mb-3">
    <%= render partial: "dosing_cohort_templates/dosing_cohorts_search_form" %>
  </div>
</div>
<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Cohort 123
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @dosing_cohorts.each do |dosing_cohort| %>
      <tr>
        <!-- Display dosing_cohort.id start -->
        <td>
          <%= dosing_cohort.id %>
        </td>
        <!-- Display dosing_cohort.id end -->

        <!-- Display dosing_cohort.cohort_123 start -->
        <td>
          <%= dosing_cohort.cohort_123 %>
        </td>
        <!-- Display dosing_cohort.cohort_123 end -->

        <!-- Display dosing_cohort.created_at start -->
        <td>
          <%= time_ago_in_words(dosing_cohort.created_at) %> ago
        </td>
        <!-- Display dosing_cohort.created_at end -->

        <!-- Display dosing_cohort.updated_at start -->
        <td>
          <%= time_ago_in_words(dosing_cohort.updated_at) %> ago
        </td>
        <!-- Display dosing_cohort.updated_at end -->

        <td>
          <a href="/dosing_cohorts/<%= dosing_cohort.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>

    <div class="row mb-3">
      <div class="col-md-12">
        <%= paginate @dosing_cohorts, theme: "twitter-bootstrap-4" %>
      </div>
    </div>

  </div>
</div>
