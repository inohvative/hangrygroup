<h1>
  Add a new glucose level
</h1>


<hr>

<!-- Form glucose_level start -->
<form action="/create_glucose_level" method="post">
<!-- Form glucose_level end -->

  <!-- Label and input for glucose_level start -->
  <div class="form-group">
    <label for="glucose_level">
      Glucose level
    </label>

    <!-- Input for glucose_level start -->
    <input type="text" id="glucose_level" name="glucose_level" class="form-control">
    <!-- Input for glucose_level end -->
  </div>
  <!-- Label and input for glucose_level end -->

  <!-- Label and input for subject_id start -->
  <div class="form-group">
    <label for="subject_id">
      Subject
    </label>

    <!-- Input for subject_id start -->
    <input type="text" id="subject_id" name="subject_id" class="form-control">
    <!-- Input for subject_id end -->
  </div>
  <!-- Label and input for subject_id end -->

  <button class="btn btn-block btn-outline-secondary">
    Create glucose level
  </button>
</form>

<hr>

<a href="/glucose_levels">
  Go back
</a>
